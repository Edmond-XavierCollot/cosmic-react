{
  "name": "cosmic-react",
  "description": "cosmic boilerplate for react apps",
  "version": "1.0.0",
  "author": "Edmond-Xavier Collot <edxcollot@gmail.com>",
  "bin": {
    "cosmic-react": "./bin/run"
  },
  "bugs": "https://github.com/git@github.com:Edmond-XavierCollot/cosmic-react.git/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "globby": "^10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/git@github.com:Edmond-XavierCollot/cosmic-react.git",
  "keywords": [
    "react",
    "cosmic",
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "cosmic-react",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "git@github.com:Edmond-XavierCollot/cosmic-react.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
